{
  "firebase": "omen",
  "public": ".",
  "ignore": [
    "**/node_modules/**",
    "firebase.json",
    "device/**",
    "device.*",
    "**/.*"
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}